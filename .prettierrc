{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "strict",
  "embeddedLanguageFormatting": "auto",
  "overrides": [
    {
      "files": ".html",
      "options": {
        "parser": "html",
        "printWidth": 80,
        "proseWrap": "always",
        "htmlWhitespaceSensitivity": "ignore"
      }
    },
    { "files": ".css", "options": { "parser": "css", "printWidth": 80 } },
    { "files": "*.js", "options": { "parser": "babel", "printWidth": 80 } }
  ]
}
